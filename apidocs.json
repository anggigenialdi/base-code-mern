  "info": {
    "title": "API V1",
    "description": "V1 APi",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://localhost:3001/v1",
      "description": "localhost"
    }
  ],
  "tags": [
    {
      "name": "Users",
      "description": "Users"
    }
  ],
  "paths": {
    "/users/add-users": {
      "post": {
        "tags": ["Users"],
        "summary": "Create Users",
        "description": "",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "fullName": {
                    "type": "string",
                    "example": "Jessica Smith"
                  },
                  "phoneNumber": {
                    "type": "string",
                    "example": "08578123412"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Created"
          },
          "404": {
            "description": "Not Found"
          }
        }
      }
    },
    "/users/login": {
      "post": {
        "tags": ["Users"],
        "summary": "Login",
        "description": "",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "phoneNumber": {
                    "type": "string",
                    "example": "085780010162"
                  },
                  "password": {
                    "type": "string",
                    "example": "123456"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Created"
          },
          "404": {
            "description": "Not Found"
          }
        }
      }
    }
  }
